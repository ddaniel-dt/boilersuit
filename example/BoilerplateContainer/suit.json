{
  "getAssessments": {
    "initialState": {
      "isLoading": false,
      "hasSucceeded": false,
      "hasError": false,
      "errorMessage": "",
      "data": null,
      "assessmentType": null
    },
    "actions": {
      "getAssessmentsStarted": {
        "saga": true,
        "passAsProp": true,
        "set": {
          "isLoading": true,
          "hasSucceeded": false,
          "hasError": false,
          "errorMessage": "",
          "data": null
        }
      },
      "getAssessmentsSucceeded": {
        "set": {
          "isLoading": false,
          "data": "payload.data",
          "hasSucceeded": false,
          "assessmentType": "payload.assessmentType"
        }
      },
      "getAssessmentsFailed": {
        "set": {
          "isLoading": false,
          "errorMessage": "Get Posts has failed",
          "hasError": true
        }
      }
    }
  }
}