{
  "getActions": {
    "initialState": {
      "isLoading": false,
      "hasCompleted": true,
      "data": []
    },
    "actions": {
      "getActionsStarted": {
        "set": {
          "isLoading": false
        }
      }
    }
  },
  "getFields": {
    "initialState": {
      "isLoading": false,
      "hasCompleted": true,
      "data": [],
      "errorMessage": ""
    },
    "actions": {
      "getFieldsStarted": {
        "passAsProp": false,
        "set": { "isLoading": true }
      },
      "getFieldsFailed": {
        "passAsProp": true,
        "set": {
          "isLoading": false,
          "data": "payload"
        }
      }
    }
  }
}
