{
  "getTweets": {
    "initialState": {
      "isLoading": false,
      "hasCompleted": false,
      "data": [],
      "hasError": false,
      "errorMessage": ""
    },
    "actions": {
      "getTweetsStarted": {
        "set": {
          "isLoading": true
        },
        "saga": {
          "onFail": "getTweetsFailed",
          "onSuccess": "getTweetsSucceeded"
        }
      },
      "getTweetsFailed": {
        "payload": true,
        "set": {
          "isLoading": false,
          "errorMessage": "payload",
          "hasError": true
        }
      },
      "getTweetsSucceeded": {
        "payload": true,
        "set": {
          "isLoading": false,
          "hasCompleted": true,
          "data": "payload"
        }
      }
    }
  }
}
